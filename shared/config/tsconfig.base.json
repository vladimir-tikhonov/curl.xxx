{
    "compilerOptions": {
      "moduleResolution": "node",
      "target": "es2016",
      "sourceMap": true,
      "pretty": true,

      "strict": true,
      "alwaysStrict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitReturns": true,
      "allowUnreachableCode": false,
      "forceConsistentCasingInFileNames": true
    }
  }
